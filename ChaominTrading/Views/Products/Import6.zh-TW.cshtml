
@{
    ViewBag.Title = "PRODUCTS SPECIES:洋蔥";
    ViewBag.BodyClass = "Import";
    ViewBag.breadCrumb = "洋蔥";
}

@section breadCrumb {
    <li><a href="~/Products/Import_list">進口</a></li>
}

<h1 class="title"><span>@ViewBag.breadCrumb</span></h1>

<section class="row row-x-0 m-b-48">
    <div class="col-6 product-info">
        <h2 class="title-sort">DESCRIPTION</h2>
        <article class="editor m-t-24">
            <ul>
                <li>品名：黃洋蔥/ 紫洋蔥</li>
                <li>產地：美國/紐西蘭</li>
                <li><strong>包裝方式：</strong><br>美國洋蔥 22.7kg(50磅)/袋<br>紐西蘭洋蔥  20kg/袋</li>
            </ul>
        </article>
    </div>
    <div class="col-6 product-pic">
        <img src="~/Content/images/products/pic-i6.jpg" alt="白蘿蔔圖片">
    </div>
</section>

<footer><a href="javascript:history.back()" class="btn icon-reply">回列表</a></footer>