
@{
    ViewBag.Title = "PRODUCTS SPECIES:Chinese Cabbage";
    ViewBag.BodyClass = "Import";
    ViewBag.breadCrumb = "CHINESE CABBAGE";
}

@section breadCrumb {
    <li><a href="~/Products/Import_list">IMPORT</a></li>
}

<h1 class="title"><span>@ViewBag.breadCrumb</span></h1>

<section class="row row-x-0 m-b-48">
    <div class="col-6 product-info">
        <h2 class="title-sort">DESCRIPTION</h2>
        <article class="editor m-t-24">
            <ul>
                <li>
                    <strong>Item:</strong>
                    Chinese Cabbage
                </li>
                <li>
                    <strong>Packing detail:</strong>
                    5~8pcs/bag, Net weight 15kg/ctn
                </li>
                <li>
                    <strong>Loading:</strong>
                    1057 or 1078 ctns/RF Container(40' RF)
                </li>
            </ul>
        </article>
    </div>
    <div class="col-6 product-pic">
        <img src="~/Content/images/products/pic-i1.jpg" alt="Chinese Cabbage">
    </div>
</section>

<footer><a href="javascript:history.back()" class="btn icon-reply">BACK</a></footer>