
@{
    ViewBag.Title = "PRODUCTS SPECIES:Baby Corn";
    ViewBag.BodyClass = "Import";
    ViewBag.breadCrumb = "BABY CORN";
}

@section breadCrumb {
    <li><a href="~/Products/Import_list">IMPORT</a></li>
}

<h1 class="title"><span>@ViewBag.breadCrumb</span></h1>

<section class="row row-x-0 m-b-48">
    <div class="col-6 product-info">
        <h2 class="title-sort">DESCRIPTION</h2>
        <article class="editor m-t-24">
            <p>Baby corn (Zea mays) is a popular Asian vegetable that can be consumed cooked or raw due to its sweet and succulent taste.Fresh baby corn cobs are best eaten soon after purchase but can be stored for up to 1 week in the salad drawer of the refrigerator.</p>
            <ul>
                <li><strong>Cultivation Regions:</strong>Thailand</li>
                <li><strong>Packing:</strong>100g/tray</li>
                <li><strong>Availability:</strong>Year round</li>
            </ul>
        </article>
    </div>
    <div class="col-6 product-pic">
        <img src="~/Content/images/products/pic-i3.jpg" alt="Baby Corn">
    </div>
</section>

<footer><a href="javascript:history.back()" class="btn icon-reply">BACK</a></footer>