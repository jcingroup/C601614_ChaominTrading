
@{
    ViewBag.Title = "PRODUCTS SPECIES:Onions";
    ViewBag.BodyClass = "Import";
    ViewBag.breadCrumb = "Onions";
}

@section breadCrumb {
    <li><a href="~/Products/Import_list">IMPORT</a></li>
}

<h1 class="title"><span>@ViewBag.breadCrumb</span></h1>

<section class="row row-x-0 m-b-48">
    <div class="col-6 product-info">
        <h2 class="title-sort">DESCRIPTION</h2>
        <article class="editor m-t-24">
            <ul>
                <li>Item：Yellow(Brown) onion / Red onion</li>
                <li><strong>Packing detail：</strong><br>US onions : Net weight 22.7kg (50 lb) /bag <br>New Zealand onions : Net weight 20kg/bag</li>
                <li><strong>Loading：</strong><br>US onions 1044~1080 bags/RF Container(40' RF) <br> New Zealand onions 1200~1300 bags/RF Container(40' RF)</li>
            </ul>
        </article>
    </div>
    <div class="col-6 product-pic">
        <img src="~/Content/images/products/pic-i6.jpg" alt="White Radish">
    </div>
</section>

<footer><a href="javascript:history.back()" class="btn icon-reply">BACK</a></footer>